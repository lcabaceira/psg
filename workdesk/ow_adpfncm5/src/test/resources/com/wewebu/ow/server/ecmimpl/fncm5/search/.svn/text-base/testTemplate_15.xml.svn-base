<?xml version="1.0" ?>
<storedsearch xmlns="http://filenet.com/namespaces/wcm/apps/1.0">
	<searchspec versionselection="releasedversion">
		<searchtype>
			<template showandorconditions="false" showmaxrecords="false"
				showoperators="false" />
		</searchtype>
		<objectstores mergeoption="union">
			<objectstore name="P8ConfigObjectStore" />
		</objectstores>
		<searchcriteria>
			<searchclauses>
				<searchclause>
					<select>
						<selectprops>
							<selectprop symname="DocumentTitle" sortlevel="1"
								sortorder="ascending" />
							<selectprop symname="DateCreated" sortlevel="2"
								sortorder="descending" />
						</selectprops>
					</select>
					<from>
						<class objecttype="document" symname="Document" />
						<class objecttype="folder" symname="Folder" />
					</from>
					<where>
						<eq>
							<whereprop editproperty="editable" symname="DocumentTitle" />
							<literal>S[woohoo]</literal>
						</eq>
					</where>
					<subclasses>
						<subclass editproperty="editable" itemid="6"
							includesubclasses="true" name="Correspondence" objecttype="document"
							symname="Email" />

						<subclass editproperty="editable" itemid="5"
							includesubclasses="false" name="Memorandum" objecttype="document"
							symname="Memo" />


						<subclass editproperty="editable" itemid="4"
							includesubclasses="true" name="Mailbox" objecttype="folder"
							symname="Inbox" />

						<subclass editproperty="editable" itemid="4" name="Notes"
							objecttype="folder" symname="Notes" />

					</subclasses>
				</searchclause>
			</searchclauses>
		</searchcriteria>
		<options maxrecords="100" />
	</searchspec>
</storedsearch>