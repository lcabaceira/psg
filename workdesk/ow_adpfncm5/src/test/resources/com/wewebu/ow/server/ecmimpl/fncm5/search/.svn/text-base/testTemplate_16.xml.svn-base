<?xml version="1.0" ?>
<storedsearch xmlns="http://filenet.com/namespaces/wcm/apps/1.0">
	<searchspec versionselection="releasedversion">
		<searchtype>
			<template showandorconditions="false" showmaxrecords="false"
				showoperators="false" />
		</searchtype>
		<searchcriteria>
			<searchclauses>
				<searchclause>
					<select>
						<selectprops>
							<selectprop symname="DocumentTitle" sortlevel="1" sortorder="ascending" />
							<selectprop symname="DateCreated" sortlevel="2"	sortorder="descending" />
						</selectprops>
					</select>
					<from>
						<class objecttype="document" symname="Document" />
						<class objecttype="folder" symname="Folder" />
					</from>
					<where>
						<eq>
							<whereprop editproperty="editable" symname="DocumentTitle" />
							<literal>S[woohoo]</literal>
						</eq>
					</where>
					<subclasses>
						<subclass editproperty="editable" itemid="6" includesubclasses="true" name="Correspondence" objecttype="document" symname="Email" />
					</subclasses>
				</searchclause>
			</searchclauses>
		</searchcriteria>
		<options maxrecords="100" />
	</searchspec>
</storedsearch>