<?xml version="1.0" encoding="UTF-8" ?>
<storedsearch>
   <searchspec versionselection="currentversion">
      <searchclauses>
         <select>
            <selectprops>
               <selectprop symname="cmis:document.cmis:name"/>
               <selectprop symname="cmis:document.cmis:lastModifiedBy"/>
               <!-- selectprop symname="cmis:name" sortlevel="1" sortorder="ascending"/>
               <selectprop symname="cmis:lastModifiedBy" sortlevel="2" sortorder="descending"/-->
            </selectprops>
         </select>
         <from>
            <class symname="cmis:document"/>
         </from>
         <where>
            <and>
            <eq>
               <whereprop editproperty="editable" symname="cmis:document.cmis:name"/>
               <literal>S[myname]</literal>
            </eq>
            <like>
               <whereprop editproperty="editable" symname="cmis:document.cmis:lastModifiedBy"/>
               <literal>T#'_/*</literal>
            </like>
            </and>
         </where>
      </searchclauses>
      <options maxrecords="100"/>
   </searchspec>
</storedsearch>