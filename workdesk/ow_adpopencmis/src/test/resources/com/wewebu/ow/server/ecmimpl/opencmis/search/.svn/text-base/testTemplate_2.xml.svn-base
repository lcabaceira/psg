<?xml version="1.0" encoding="UTF-8" ?>
<storedsearch>
	<searchspec versionselection="currentversion">
		<searchclauses>
			<select>
				<selectprops>
					<selectprop symname="cmis:document.cmis:name" sortlevel="1" sortorder="ascending" />
					<selectprop symname="cmis:document.cmis:lastModifiedBy" sortlevel="2"
						sortorder="descending" />
				</selectprops>
			</select>
			<from>
				<class symname="cmis:document" />
			</from>
			<where>
				<or>
					<eq>
						<whereprop editproperty="editable" symname="cmis:document.cmis:personalNumber" />
						<literal>L[2231233]</literal>
					</eq>
					<isin>
						<whereprop editproperty="editable" symname="cmis:document.cmis:departmentName" />
						<literal>S[HR,AC,mobile]</literal>
					</isin>
				</or>
				<and>
				    <isnotin>
                        <whereprop editproperty="editable" symname="cmis:document.cmis:officeNumber" />
                        <literal>L[{2231,999,654}]</literal>
                    </isnotin>
				</and>
			</where>
		</searchclauses>
		<options maxrecords="100" />
	</searchspec>
</storedsearch>