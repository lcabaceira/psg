<?xml version="1.0" encoding="UTF-8" ?>
<storedsearch>
	<searchspec versionselection="currentversion">
		<searchclauses>
			<select>
				<selectprops>
					<selectprop symname="cmis:document.cmis:name" sortlevel="1"
						sortorder="ascending" />
					<selectprop symname="ws:article.ws:journal" sortlevel="2"
						sortorder="ascending" />
					<selectprop symname="ws:blogPost.ws:url" sortlevel="3"
						sortorder="ascending" />
				</selectprops>
			</select>
			<from>
				<class symname="cmis:document" />
			</from>
			<where>
				<and>
                    <like>
                        <whereprop editproperty="editable" symname="ws:paper.ws:synopsis" />
                        <literal>S[Liver]</literal>
                    </like>
					<or>
						<like>
							<whereprop editproperty="editable" symname="ws:article.ws:journal" />
							<literal>S[Medicine Today]</literal>
						</like>
						<like>
							<whereprop editproperty="editable" symname="ws:blogPost.ws:url" />
							<literal>S[http://blog.b.log]</literal>
						</like>

					</or>
				</and>
			</where>
			<subclasses>
				<subclass objecttype="document" name="Article" symname="ws:article" />
				<subclass objecttype="document" name="Journal" symname="ws:blogPost" />
			</subclasses>
		</searchclauses>
		<options maxrecords="100" />
	</searchspec>
</storedsearch>