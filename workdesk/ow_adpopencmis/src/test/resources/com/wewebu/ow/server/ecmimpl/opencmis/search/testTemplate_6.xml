<?xml version="1.0" encoding="UTF-8" ?>
<storedsearch>
	<searchspec versionselection="currentversion">
		<searchclauses>
			<select>
				<selectprops>
					<selectprop symname="cmis:document.cmis:name" sortlevel="1" sortorder="ascending" />
					<selectprop symname="cmis:document.cmis:lastModifiedBy" sortlevel="2"
						sortorder="descending" />
				</selectprops>
			</select>
			<from>
				<class symname="cmis:document" />
			</from>
			<where>
			    <and>
    			    <isnull>
	       			   <whereprop editproperty="editable" symname="cmis:document.cmis:name" />
				    </isnull>
				    <isnotnull>
                       <whereprop editproperty="editable" symname="cmis:document.cmis:officeNumber" />
                    </isnotnull>
				</and>
			</where>
			
		</searchclauses>
		<options maxrecords="100" />
	</searchspec>
</storedsearch>