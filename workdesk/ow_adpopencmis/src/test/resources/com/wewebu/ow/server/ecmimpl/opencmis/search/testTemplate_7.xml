<?xml version="1.0" encoding="UTF-8" ?>
<storedsearch>
	<searchspec versionselection="currentversion">
		<searchclauses>
			<select>
				<selectprops>
					<selectprop symname="cmis:document.cmis:name" sortlevel="1" sortorder="ascending" />
					<selectprop symname="cmis:document.cmis:lastModifiedBy" sortlevel="2"
						sortorder="descending" />
				</selectprops>
			</select>
			<from>
				<class symname="cmis:document" />
			</from>
			<where>
				<or>
					<in>
						<whereprop editproperty="editable" symname="cmis:document.cmis:topics" />
						<literal>S[{financial,math}]</literal>
					</in>
					<notin>
                        <whereprop editproperty="editable" symname="cmis:document.cmis:topics" />
                        <literal>S[{RedRindingHood,Robin Hood}]</literal>
                    </notin>
                    <isin>
                        <whereprop editproperty="editable" symname="cmis:document.cmis:topics" />
                        <literal>S[{delivery,provisioning}]</literal>
                    </isin>
                    <isnotin>
                        <whereprop editproperty="editable" symname="cmis:document.cmis:topics" />
                        <literal>S[{delivery,provisioning}]</literal>
                    </isnotin>
				</or>
			</where>
		</searchclauses>
		<options maxrecords="100" />
	</searchspec>
</storedsearch>