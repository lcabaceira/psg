package com.wewebu.ow.server.ui.preview;

import com.wewebu.ow.server.ecm.OwObject;
import com.wewebu.ow.server.ui.OwAppContext;

/**
 *<p>
 * Interface for the context of integrated preview. 
 *</p>
 *
