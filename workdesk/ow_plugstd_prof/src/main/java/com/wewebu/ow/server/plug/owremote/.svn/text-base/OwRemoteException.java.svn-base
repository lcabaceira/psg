package com.wewebu.ow.server.plug.owremote;

import com.wewebu.ow.server.exceptions.OwException;
import com.wewebu.ow.server.util.OwString;

/**
 *<p>
 * OwRemote Exception used when calling a document by URL.
 *</p>
