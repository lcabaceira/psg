<?xml version="1.0" ?>
<storedsearch>


	<searchspec versionselection="releasedversion">


		<searchcriteria>
			<searchclauses>

				<searchclause>


			<select>
				<selectprops>
					<selectprop symname="OW_ClassDescription" />
					<selectprop symname="DateCreated" />
					<selectprop symname="OW_HIST_ModifiedProperties" />
					<selectprop symname="Creator" />
				</selectprops>
			</select>

			<from>
				<class symname="Event"/>
			</from>

			<where>
				<and>
					<like>
						<whereprop editproperty="editable" symname="Creator">
						</whereprop>
						<literal />
					</like>
					<gte>
						<whereprop editproperty="editable"  symname="DateCreated">
						</whereprop>
						<literal />
					</gte>
					<lte>
						<whereprop editproperty="editable" symname="DateCreated">
						</whereprop>
						<literal />
					</lte>
				</and>
			</where>

				</searchclause>

			</searchclauses>

		</searchcriteria>
		<options maxrecords="100" />
	</searchspec>
</storedsearch>