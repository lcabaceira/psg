<?xml version="1.0" ?>
<storedsearch>
	<searchspec versionselection="currentversion">
		<searchclauses>
			<select>
				<selectprops>
					<selectprop symname="cmis:name" />
					<selectprop symname="cmis:modifiedBy" />
				</selectprops>
			</select>
			<from>
				<class symname="document" />
			</from>
			<where>
				<and>
					<like>
						<whereprop editproperty="editable" symname="cmis:name">
						</whereprop>
						<literal />
					</like>
				</and>
			</where>
		</searchclauses>
		<options maxrecords="100" />
	</searchspec>
</storedsearch>