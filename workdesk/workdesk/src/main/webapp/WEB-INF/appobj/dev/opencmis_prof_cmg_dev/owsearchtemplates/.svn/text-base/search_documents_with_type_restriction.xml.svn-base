<?xml version="1.0" encoding="UTF-8" ?>
<storedsearch>
	<searchspec versionselection="currentversion">
		<searchclauses>
			<select>
				<selectprops>
					<selectprop symname="cmis:document.cmis:name" sortlevel="1"
						sortorder="ascending" />
					<selectprop symname="cmis:document.cmis:lastModifiedBy"
						sortlevel="2" sortorder="descending" />
				</selectprops>
			</select>
			<from>
				<class symname="document" />
			</from>
			<where>
				<and>
					<like>
						<whereprop editproperty="editable" symname="cmis:document.cmis:name" />
					</like>
					<between>
						<whereprop editproperty="editable"
							symname="cmis:document.cmis:lastModificationDate" />
					</between>
				</and>
				<or>
					<eq>
						<whereprop editproperty="readonly"
							symname="cmis:document.cmis:objectTypeId" />
						<literal>D:owd:hrdocument</literal>
					</eq>
					<eq>
						<whereprop editproperty="readonly" uniquename="class2"
							symname="cmis:document.cmis:objectTypeId" />
						<literal>D:owd:hrimage</literal>
					</eq>
				</or>
			</where>
			<subclasses>
				<subclass objecttype="document" name="Thumbnail" symname="cmis:document" />
			</subclasses>
		</searchclauses>
		<options maxrecords="100" />
	</searchspec>
</storedsearch>