<?xml version="1.0" ?>
<storedsearch>
    <searchspec versionselection="releasedversion">
        <searchclauses>
            <select>
                <selectprops>
                    <selectprop symname="LastName" />
                    <selectprop symname="FirstName" />
                    <selectprop symname="FaxNumber" />
                </selectprops>
            </select>
            <where>
                <and>
                    <like>
                        <whereprop editproperty="editable" symname="LastName" />
                        <literal>%</literal>
                    </like>
                </and>
            </where>
        </searchclauses>
        <options maxrecords="100" />
    </searchspec>
</storedsearch>