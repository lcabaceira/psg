<?xml version="1.0" ?>
<VirtualFolderDescription>

	<!-- (optional) java class name of virtual folder factory to use. Leave 
		empty to use standard: com.wewebu.ow.server.ecm.OwStandardVirtualFolderObjectFactory -->
	<ClassName>com.alfresco.ow.contractmanagement.virtualfolder.OwContractManagementVirtualFolderObjectFactory</ClassName>

	<root name="Contract" containsdocs="true">
		<search versionselection="currentversion">
			<!-- currentversion,allversions,releasedversion,checkedoutversion,inprocessversion,majorversions,minorversions -->
			<objectstores mergeoption="union">
				<objectstore name="Main Repository" />
			</objectstores>
			<select>
				<selectprops>
					<selectprop symname="cmis:document.cmis:name" />
					<selectprop symname="cmis:document.cmis:creationDate" />
					<selectprop symname="cmis:document.cmis:createdBy" />
					<selectprop symname="cmis:document.cmis:objectTypeId" />
				</selectprops>
			</select>
			<from>
				<class symname="document" />
			</from>
		</search>

		<node name="Search for documents" mimetype="ow_folder/search" containsdocs="true">
			<search versionselection="currentversion">
				<select>
					<selectprops>
						<selectprop symname="cmis:document.cmis:name" />
						<selectprop symname="cmis:document.cmis:creationDate" />
						<selectprop symname="cmis:document.cmis:lastModificationDate" />
						<selectprop symname="cmis:document.cmis:createdBy" />
					</selectprops>
				</select>
				<from>
					<class symname="document" />
				</from>
				<where>
					<and>
						<cbr_in>
							<whereprop editproperty="editable" symname="cmis:document.cmis:name" />
						</cbr_in>
						<between>
							<whereprop editproperty="editable" symname="cmis:document.cmis:creationDate" datemode="ignoretime" />
							<literal />
						</between>
						<between>
							<whereprop editproperty="editable"
								symname="cmis:document.cmis:lastModificationDate" datemode="ignoretime" />
							<literal />
						</between>
					</and>
				</where>
				<subclasses>
					<subclass editproperty="hidden" name="Document"
						objecttype="document" symname="D:cmg:document" />
				</subclasses>
			</search>
		</node>
		<reference name="Contracts" mimetype="ow_folder/F:cmg:contractReferences" direction="INBOUND">
			<linkclasses>
      			<name>R:cmg:partners</name>
      			<name>R:cmg:responsiblePersons</name>
    		</linkclasses>
			<search versionselection="currentversion">
				<select>
					<selectprops>
						<selectprop symname="cmis:folder.cmis:name" />
					</selectprops>
				</select>
			</search>
		</reference>
	</root>
</VirtualFolderDescription>