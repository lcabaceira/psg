  <!-- ================================================================================

     PLUGIN-CLASS: [...plug.owefileref.OwEFileReferenceDocumentFunction]

     Document Plugin to open associated eFile.
     Opens an eFile, which is directly associated with the initial source object, such as a task object.

  ==================================================================================-->
  <PlugIn type="ow_docfunction">
    <Name>Open Associated eFile</Name>
    <id>com.wewebu.owefileref.open</id>
    <Description>Opens an eFile, which is directly associated with the initial source object, such as a task object</Description>
    <Vendor>Alfresco Business Solutions</Vendor>
    <Version>4.2.0</Version>
    <ClassName>com.wewebu.ow.server.plug.owefileref.OwEFileReferenceDocumentFunction</ClassName>
    <helppath>owefileref/default.jsp</helppath>
    <!-- Optional small (16x16 pixels) icon for the plugin, leave empty to use default. -->
    <icon />
    <!-- Optional small (24x24 pixels) icon for the plugin, leave empty to use default. -->
    <iconbig />
    <!-- Allow multiple document selection or just one single document -->
    <Multiselect>false</Multiselect>
    <!-- Display the plugin in the context menu. -->
    <ContextMenu>true</ContextMenu>
    <!-- Display in object editing dialogs like OwEditPropertiesDialog. -->
    <ShowInEditViews>true</ShowInEditViews>
    <!-- Display the plugin next to object in a object listview. -->
    <ObjectInstance>true</ObjectInstance>
    <!-- Create a own column for plugin with given title, or display without columntitle. -->
    <ColumnTitle />
    <!-- Object types to work on as defined in OwObjectReference.OBJECT_TYPE_...-->
    <objecttypes>
      <type>OBJECT_TYPE_DOCUMENT</type>
      <type>OBJECT_TYPE_FOLDER</type>
    </objecttypes>
    <!-- Enable plugin only for the given object class names, leave empty to enable for all classes -->
    <objectclasses>
      <name>D:owd:cmdocument</name>
      <name>F:owd:cmtask</name>
    </objectclasses>
    <!--
    EFile definitions
    Alternatively the eFile can be found by using a FieldControl:
    When clicking in the "Edit Properties" or in "Result List" Views on the value of a
    property with name CMEObjectID, the object of type eFile with the same value for the
    property with the name CMEFileID gets displayed under the condition that only one
    object with this ID exists.
    <FieldControl fieldclass="com.wewebu.ow.server.plug.owrecordext.fieldcontrol.OwFieldManagerControlForeignKey" objecttype="CMEObjectID">
        <ForeignObjectDefinition objectclass="CMEFile" key="CMEFileID" />
    </FieldControl>
    -->
    <EFiles>
      <!-- An EFile definition -->
      <EFile>
        <!-- restrict the object class for which the refinement should be handled -->
        <objectclasses>
          <name>F:owd:cmtask</name>
        </objectclasses>
        <!-- Definition of the search template which is required to search for the related eFile.
        It may contain just the search fields listed with the <PropertyMapping> element.
        Please note:
        * This template needs to be stored in the "other" folder of the configuration
        * This template is different from the one used to define the virtual structure of the eFile -->
        <Search templatename="Find_CM_eFiles_by_ID.xml">
          <PropertyMapping efileproperty="P:owd:cmefileIdAspect.owd:cmefileId" objectproperty="P:owd:cmefileIdAspect.owd:cmefileId" />
        </Search>
        <!-- Refinement to jump to the specific subpath of the semi-virutal folder -->
        <Refinement subpath="Accounting">
          <objectclasses>
            <name>F:owd:cmtask</name>
          </objectclasses>
          <expression><![CDATA[object.owd\:Assignee='Accounting']]></expression>
        </Refinement>
        <Refinement subpath="Sales">
          <objectclasses>
            <name>F:owd:cmtask</name>
          </objectclasses>
          <expression><![CDATA[object.owd\:Assignee='Sales']]></expression>
        </Refinement>
      </EFile>
      <EFile>
        <objectclasses>
          <name>D:owd:cmdocument</name>
        </objectclasses>
        <Search templatename="Find_CM_eFiles_by_ID.xml">
          <PropertyMapping efileproperty="P:owd:cmefileIdAspect.owd:cmefileId" objectproperty="P:owd:cmefileIdAspect.owd:cmefileId" />
        </Search>
        <Refinement subpath="Accounting">
          <objectclasses>
            <name>D:owd:cmdocument</name>
          </objectclasses>
          <expression><![CDATA[object.owd\:Assignee='Accounting']]></expression>
        </Refinement>
        <Refinement subpath="Sales">
          <objectclasses>
            <name>D:owd:cmdocument</name>
          </objectclasses>
          <expression><![CDATA[object.owd\:Assignee='Sales']]></expression>
        </Refinement>
      </EFile>
    </EFiles>
  </PlugIn>
