  <!-- ====================================================

		PLUGIN-CLASS: [...owdocprops.OwEditDocumentPropertiesFunction]

		Document plugin that is used to edit objects.

		It provides several views to view and edit object properties,
		like metadata, history, versions...
		The different views can be switched on or off with the configuration.

		NOTE:	For the FiledInView to work,
			you need to specifiy the FolderClass ID
			in the bootstrap section: <RecordClasses>

	===================================================== -->
  <PlugIn type="ow_docfunction">
    <Name>Show history</Name>
    <id>com.wewebu.ow.owviewhistory</id>
    <Description>Open the Show History dialog</Description>
    <Vendor>Alfresco Business Solutions</Vendor>
    <Version>4.2.0</Version>
    <ClassName>com.wewebu.ow.server.plug.owdocprops.OwEditDocumentPropertiesFunction</ClassName>
    <!-- optional small (16x16 pixels) icon for the plugin, leave empty to use default. -->
    <icon>/images/plug/owdocprops/history.png</icon>
    <!-- optional small (24x24 pixels) icon for the plugin, leave empty to use default. -->
    <iconbig>/images/plug/owdocprops/history_24.png</iconbig>
    <!-- Allow multiple document selection or just one single document -->
    <Multiselect>false</Multiselect>
    <!-- Display the plugin in the context menu. -->
    <ContextMenu>false</ContextMenu>
    <!-- Display in object editing dialogs like OwEditPropertiesDialog. -->
    <ShowInEditViews>false</ShowInEditViews>
    <!-- Display the plugin next to object in a object listview. -->
    <ObjectInstance>true</ObjectInstance>
    <!-- Create a own column for plugin with given title, or display without columntitle. -->
    <ColumnTitle />
    <!-- true = the latest version is edited, even if a older version was opened, false = edit the selected version -->
    <AlwaysUseLatestVersion>true</AlwaysUseLatestVersion>
    <!-- Determines the views to be displayed-->
    <!-- View for Properties to display and edit, read-only (set View read-only)-->
    <PropertiesView readonly="false">false</PropertiesView>
    <!-- View for system properties to display, read-only (is default always)-->
    <SystemPropertiesView>false</SystemPropertiesView>
    <!-- View for Accessrigths to display and edit, read-only (set View read-only)-->
    <AccessRightsView readonly="false">false</AccessRightsView>
    <!-- View to display where the file is contained, read-only (set View read-only)-->
    <FiledInView>false</FiledInView>
    <!-- View to display the Version history of file, read-only (set View read-only)-->
    <VersionsView>false</VersionsView>
    <!-- View to display change history of file and metadata, read-only (set View read-only)-->
    <HistoryView objecthistory="true" casehistory="false">true</HistoryView>
    <!-- Should the Documentfunctions be rendered -->
    <DocumentFunctionsView>false</DocumentFunctionsView>
    <!-- special behaviors -->
    <!-- enable the paste button for metadata from clipboard -->
    <EnablePasteMetadata>false</EnablePasteMetadata>
    <!-- Optional single default searchtemplate name to be used for the history view.
			If empty, do not use a search template and display all history entries.
			The location of the single searchtemplate depends on the ECM-Adapter. (see corresponding documentation) -->
    <HistoryViewSearchTemplate />
