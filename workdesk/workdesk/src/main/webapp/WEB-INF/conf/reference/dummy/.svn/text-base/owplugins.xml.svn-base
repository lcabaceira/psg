  <!-- ====================================================

		PLUGIN-CLASS: [...owdocprops.OwEditDocumentPropertiesFunction]

		Document plugin that is used to edit objects.

		It provides several views to view and edit object properties,
		like metadata, history, versions...
		The different views can be switched on or off with the configuration.

		NOTE:	For the FiledInView to work,
			you need to specifiy the FolderClass ID
			in the bootstrap section: <RecordClasses>

	===================================================== -->
  <PlugIn type="ow_docfunction">
    <Name>Edit properties with property grouping</Name>
    <id>com.wewebu.ow.owdocprops.group</id>
    <Description>Edit properties with property grouping</Description>
    <Vendor>Alfresco Business Solutions</Vendor>
    <Version>4.2.0</Version>
    <ClassName>com.wewebu.ow.server.plug.owdocprops.OwEditDocumentPropertiesFunction</ClassName>
    <helppath>owdocprops/default.jsp</helppath>
    <!-- optional small (16x16 pixels) icon for the plugin, leave empty to use default. -->
    <icon />
    <!-- optional small (24x24 pixels) icon for the plugin, leave empty to use default. -->
    <iconbig />
    <!-- Allow multiple document selection or just one single document -->
    <Multiselect>true</Multiselect>
    <!-- Display the plugin in the context menu. -->
    <ContextMenu>true</ContextMenu>
    <!-- Display in object editing dialogs like OwEditPropertiesDialog. -->
    <ShowInEditViews>false</ShowInEditViews>
    <!-- Display the plugin next to object in a object listview. -->
    <ObjectInstance>true</ObjectInstance>
    <!-- Create a own column for plugin with given title, or display without columntitle. -->
    <ColumnTitle />
    <!-- automatically open the document
            previewmode : true  open the document according to browsers default MIME-table (fast preview mode)
                          false open the document according to AWDs MIME-table defined in owmimetable.xml (normal preview mode)
		 -->
    <AutoOpen previewmode="false">true</AutoOpen>
    <!-- true = the latest version is edited, even if a older version was opened, false = edit the selected version -->
    <AlwaysUseLatestVersion>true</AlwaysUseLatestVersion>
    <!-- Determines the views to be displayed. -->
    <!-- View for Properties to display and edit, read-only (set View read-only)-->
    <PropertiesView readonly="false">true</PropertiesView>
    <!-- View for system properties to display, read-only (is default always)-->
    <SystemPropertiesView>true</SystemPropertiesView>
    <!-- View for Accessrigths to display and edit, read-only (set View read-only)-->
    <AccessRightsView readonly="false">false</AccessRightsView>
    <!-- View to display where the file is contained, read-only (set View read-only)-->
    <FiledInView>true</FiledInView>
    <!-- View to display the Version history of file, read-only (set View read-only)-->
    <VersionsView>true</VersionsView>
    <!-- View to display change history of file and metadata, read-only (set View read-only)-->
    <HistoryView objecthistory="true" casehistory="false">true</HistoryView>
    <!-- Should the Documentfunctions be rendered -->
    <DocumentFunctionsView>true</DocumentFunctionsView>
    <!-- special behaviors -->
    <!-- enable the paste button for metadata from clipboard -->
    <EnablePasteMetadata>true</EnablePasteMetadata>
    <!-- Optional single default searchtemplate name to be used for the history view.
			If empty, do not use a search template and display all history entries.
			The location of the single searchtemplate depends on the ECM-Adapter. (see corresponding documentation) -->
    <HistoryViewSearchTemplate />
    <!-- Max number of items to be displayed in the resultlists for history, version and filed in view. -->
    <MaxChildSize>50</MaxChildSize>
    <!-- enable save all button option when more than one documents are selected -->
    <EnableSaveAll>false</EnableSaveAll>
    <!--  
        Object Link View
        Attributes:    
            show        = boolean display flag
            typeList    = boolean flag that whether links will be displayed in 
                          individual link class tables
            direction   = INBOUND|OUTBOUND|BOTH link direction/relation to be displayed     
    -->
    <LinksView show="true" typeList="false" direction="BOTH">
      <!-- Link object class names that the link view will display -->
      <linkclasses>
        <name>Parent</name>
        <name>Self</name>
      </linkclasses>
    </LinksView>
    <!-- Object types to work on as defined in OwObjectReference.OBJECT_TYPE_... -->
    <objecttypes>
      <type>OBJECT_TYPE_DOCUMENT</type>
      <type>OBJECT_TYPE_FOLDER</type>
    </objecttypes>
    <!-- List of the Properties for Batch-Indexing,
			i.e. property value is submitted to the next pane when multiple documents are edited
		-->
    <EditBatchPropertyList>
      <property>ow_Filename</property>
    </EditBatchPropertyList>
    <!-- Enable plugin only for the given object class names, leave empty to enable for all classes -->
    <objectclasses>
      <!--<name>document</name>-->
    </objectclasses>
    <!--
        Property restriction definition
        property: the id/symbolic name of property to display
          Attribute readonly: controlling if it should be readonly in UI (ECM system definition will be kept)
        Group the properties.
            Attributes:
                *collapsed*     - specify how a group will be rendered (collapsed if true or expanded if false)
                *displayname*   - the default name of the group, rendered in the HTML page. The value of *displayname*
                                can be owerwritten using user language by configuring a key that match the pattern
                                <plugin-id>.<group-id> in oecmlocalize_??.properties file
                *id*            - the group id, used as HTML id. The group ID needs to match the pattern: ([a-zA-Z])(([0-9a-zA-Z])|_)*
    -->
    <EditPropertyList>
      <PropertyGroup id="group1" displayname="Group 1" collapsed="false">
        <property readonly="true">ow_Filename</property>
        <property readonly="false">Notes</property>
      </PropertyGroup>
      <PropertyGroup id="group2" displayname="Group 2" collapsed="false">
        <property readonly="false">EvaluationPoints</property>
        <property readonly="false">FirstName</property>
        <property readonly="true">LastName</property>
      </PropertyGroup>
      <PropertyGroup id="group3" displayname="Group 3" collapsed="true">
        <property readonly="false">ComplexPerson</property>
        <property readonly="false">ComplexPersonArray</property>
      </PropertyGroup>
      <PropertyGroup id="group4" displayname="Group 4" collapsed="true">
        <property readonly="false">Address</property>
        <property readonly="false">TelNumber</property>
        <property readonly="false">FaxNumber</property>
      </PropertyGroup>
      <PropertyGroup id="group5" displayname="Group 5" collapsed="true">
        <property readonly="false">Department</property>
        <property readonly="false">EvaluationPoints</property>
        <property readonly="false">TelNumber</property>
        <property readonly="false">FaxNumber</property>
      </PropertyGroup>
    </EditPropertyList>
    <settingsset>
      <!-- Default column info for the versions view. -->
      <VersionViewColumnInfo type="com.wewebu.ow.server.settingsimpl.OwSettingsPropertyStringConfigurationBox" scope="user" edit="true" list="true" displayname="Version Columns" helppath="">
        <!--  Example for Dummy Adapter -->
        <property>ow_Filename</property>
        <property>ow_Last-Modified</property>
        <!--  Example for Dummy Adapter-->
        <comboselect displayname="Titel">ow_Filename</comboselect>
        <comboselect displayname="Letztes Schreibdatum">ow_Last-Modified</comboselect>
      </VersionViewColumnInfo>
      <!-- Default column info for the history view. -->
      <HistoryViewColumnInfo type="com.wewebu.ow.server.settingsimpl.OwSettingsPropertyString" scope="user" edit="true" list="true" displayname="History Columns" helppath="">
        <property>OW_HIST_Name</property>
        <property>OW_HIST_Summary</property>
        <property>OW_HIST_User</property>
        <property>OW_HIST_Time</property>
        <property>OW_HIST_ModifiedProperties</property>
        <!--
        <property>OW_HIST_Status</property>
        <property>OW_HIST_Type</property>
        <property>OW_HIST_Objects</property>
        <property>OW_HIST_Parent</property>
        <property>OW_HIST_ID</property>
         -->
        <!-- additional columns for FNCM History Manager -->
        <!-- <property>SourceObject</property> -->
        <!-- additional columns for FNBPM History Manager -->
        <!-- <property>F_WorkFlowNumber</property> -->
        <!-- <property>F_Queue</property> -->
        <!-- <property>F_WorkflowName</property> -->
        <!-- <property>F_Text</property> -->
        <!-- <property>F_InstSheetName</property> -->
        <!-- <property>F_MapName</property> -->
        <!-- <property>F_WorkClassName</property> -->
        <!-- <property>F_Tag</property> -->
      </HistoryViewColumnInfo>
    </settingsset>
  </PlugIn>
