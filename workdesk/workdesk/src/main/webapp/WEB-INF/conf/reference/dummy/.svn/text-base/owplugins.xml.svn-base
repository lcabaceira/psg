        <comboselect displayname="Creation date">F_CreateTime</comboselect>
        <comboselect displayname="Subject">F_Subject</comboselect>
        <comboselect displayname="Locked">F_Locked</comboselect>
        <comboselect displayname="Editor">F_LockUser</comboselect>
        <comboselect displayname="Resubmit date">OW_RESUBMIT_DATE</comboselect>
