    <!-- optional small (24x24 pixels) icon for the plugin, leave empty to use default. -->
    <iconbig />
    <!-- enable this plugin to be a drag / drop target -->
    <DragDropTarget multifile="true">false</DragDropTarget>
    <!-- enable the paste button for metadata from clipboard -->
    <EnablePasteMetadata>true</EnablePasteMetadata>
    <!-- enable the access rights page -->
    <AccessRightsView>true</AccessRightsView>
    <!-- display the available checkin modes (e.g. autoclassification) -->
    <CheckInModeOption>false</CheckInModeOption>
    <!-- display the checkbox so that user can decide to check in as major or minor version -->
    <CheckInReleaseVersionOption>true</CheckInReleaseVersionOption>
    <!-- default setting of new version: true = release (major) version, false = minor version -->
    <CheckInReleaseVersionDefault>true</CheckInReleaseVersionDefault>
    <!-- enable save all button option when more than one documents are selected -->
    <EnableSaveAll>true</EnableSaveAll>
    <!-- enable document preview -->
    <AutoOpen>true</AutoOpen>
    <!-- List of object classes, user can select from, for the object to be created 
         0 to n <ObjectClass> subnodes, defining the object classes available for selection.
         Each <ObjectClass> subnode can have attribute includeSubClasses, which can be true or false:
            - If true, all subclasses of specified object class are available for user selection.
            - If false (default if attribute is missing), only the defined object class can be selected by users.            
         If more than one <ObjectClass> subnode exists, a user selection tree will be built from all configured object classes.
    -->
    <ObjectClassSelection />
    <!-- Parent folderobject where the new document should be a child of.
        If empty the selected folder will be parent.
        Define a parent object using the following syntax.
            - PATH      e.g.:       /                (whole ECM domain, root folder)
            - PATH      e.g.:       /DevStore1/MyFolder/ (some subfolder within repository "DevStore1")
     -->
    <ParentObject />
    <!-- list of document importer -->
    <DocumentImporter>
      <Importer>
        <ClassName>com.wewebu.ow.server.docimport.OwLocalFileDocumentImporter</ClassName>
      </Importer>
      <Importer>
        <ClassName>com.wewebu.ow.server.docimport.OwNoContentDocumentImporter</ClassName>
      </Importer>
      <!-- <Importer>
          <ClassName>com.wewebu.ow.server.docimport.OwLotusDocumentActiveXImporter</ClassName>
      </Importer> -->
    </DocumentImporter>
    <!-- Define Order of property propagation, importer before or after batch properties
    propagation, per default it is false
    <ImporterBeforeBatch>false</ImporterBeforeBatch>
    Which value should be used for name property, the proposed name (without extension)
    or display name (with extension)
    <UseProposedName>true</UseProposedName> -->
    <!-- List of the Properties for Batch-Indexing,
					i.e. property value is submitted to the next pane when multiple documents are edited
				-->
    <EditBatchPropertyList>
      <property>EmployeeNumber</property>
    </EditBatchPropertyList>
    <!-- list of property names to edit. comment out to edit all properties.
                     readonly:   true = display the property read-only -->
    <!--
	<EditPropertyList>
    	<property readonly="false">EmployeeNumber</property>
	</EditPropertyList>
    -->
    <!-- Enable plugin only for the given object class names of the parent, leave empty to enable for all classes -->
    <objectclasses>
      <!--<name></name>-->
    </objectclasses>
    <!--  Define custom JspPage used for rendering objectclasses,
    If JspForms configuration is  absent the standard property renderer will be used. -->
    <JspForms>
      <JspForm>
        <JspPage />
        <objectclasses>
          <name />
        </objectclasses>
      </JspForm>
    </JspForms>
    <!-- List of objectclass mappings to be used.
                        Each objectclass is mapped to a folder.

                        Where the direction attribute will specify the following:

                                autoselectclass:        The selected subfolder will select
                                                        a objectclass to be used.


                                autoselectfolder:       The selected class will select / create
                                                        a subfolder to be used to store the new object.

                -->
    <ObjectClassMapping direction="autoselectclass">
      <!--
                                Syntax (autoselectclass): <Map>[Selected FolderName | Selected FolderClassName]=[Created ObjectClassName]</Map>
                                Example (autoselectclass): <Map>Depot=DepotDocuments</Map>

                                        Selecting the subfolder "Depot"
                                        will select the objectclass "DepotDocuments"
                                        to be used for the new object.

                                Syntax (autoselectfolder): <Map>[Created Subfolderpath]=[Selected ObjectClassName]</Map>
                                Example (autoselectfolder): <Map>Schaden-Leistungsfallakt/Schadensmeldung=Schadensmeldung</Map>

                                		Selecting a objectclass "Schadensmeldung"
                                		will create a subfolder Schaden-Leistungsfallakt/Schadensmeldung
                                		and store the new object there.

                        -->
      <!--<Map>Depot=DepotDocuments</Map>-->
    </ObjectClassMapping>
    <!-- List of property mappings to be used.
                        Each property from the parent object is set to the property in the new Object.
                        Use the . operator to resolve object reference properties.
                -->
    <ParameterMapping>
      <!--
            Define parent properties based values for the  properties  of the newly created object
            through <Map> definitions.
            A map definition consist of an equation between a property from the newly created object (left-side)
            and an expression based on properties from the parent object (right-side).
            Object reference, zero indexed array elements and complex properties references are supported
            in the right side expression.

            Examples:

            Simple mapping
            <Map>Topic=Description</Map>

            Object reference
            <Map>Topic=Description.Topic</Map>

            Array reference
            <Map>Topic=Descriptions[1]</Map>

            Array object reference or complex property
            <Map>Topic=Descriptions[2].Topic</Map>
            -->
    </ParameterMapping>
  </PlugIn>
