/* .notes  */
/* ---------------------------------- */
.notesContainer,
.notesContainer_short
{
    clear: left;
    float: left;
    max-height: 300px;
    height: expression(this.scrollHeight>299?"300px":"auto");
    overflow-y: auto;
    overflow-x: hidden;
    min-width: 350px;
    width: 100%;
}

.noteResumee
{
    float:left;
}
.noteHeader, .noteHeader_short
{
    clear:left;
    float:left;
    border-bottom:1px solid black;
    font-weight:bold;
    min-width:350px;
}
.noteContent, .noteContent_short
{
    clear:left;
    float:left;
    min-width:350px;
    width:100%;
    padding-top:5px;
    padding-bottom:10px;
    white-space:normal;
}
/*short notes */
.notesContainer_short
{
    min-width:200px;
    width:230px;
}
.noteHeader_short
{
    min-width:200px;
}
.noteContent_short
{
    min-width:200px;
    padding-top:2px;
    padding-bottom:5px;
    width:100%;
}

.OwTooltip .notesContainer,
.OwTooltip .notesContainer_short,
.OwTooltip .x-panel .noteHeader,
.OwTooltip .x-panel .noteHeader_short,
.OwTooltip .x-panel .noteContent,
.OwTooltip .x-panel .noteContent_short
{
    float: none;
}

