/* Demo Pages - this styles are for the demo pages */
#OwDemoForm
{
    background-color: white;
    padding: 15px;
}
#OwDemoForm #OwDemoForm_Header
{
    overflow: hidden;
    height: 70px;
    border-bottom: 10px solid #ECECEC;
    margin-bottom: 20px;
}
#OwDemoForm #OwDemoForm_Header h1
{
    font-size: 1.8em;
    line-height: 1.3em;
    width: 400px;
    float: left;
    font-weight: normal;
}
#OwDemoForm #OwDemoForm_Header img
{
    float: right;
}
#OwDemoForm .block
{
    border-bottom: solid 2px #ECECEC;
    margin: 0;
}
#OwDemoForm .left,
#OwDemoForm .right
{
    padding: 10px;
}
#OwDemoForm .left
{
    width: 200px;
    float: left;
    border-right: solid 2px #ECECEC;
}
#OwDemoForm .left h2
{
    font-size: 1.4em;
    font-weight: bold;
}
#OwDemoForm .OwFormTable_Column
{
    padding: 50px;
    float: left;
    margin-left: -2px;
}
#OwDemoForm .right
{
    float: left;
    border-left: solid 2px #ECECEC;
    margin-left: -2px;
}
#OwDemoForm .owdemoprop {
	padding-top:0px;
	padding-bottom:15px;
}

#OwDemoForm .info
{
    color: #888888;
    margin-top: 5px;
    display: block;
    font-size: 0.8em;
}
#OwDemoForm td.OwPropertyName
{
    width: 100px;
    padding-right: 10px; 
}
#OwDemoForm_Footer
{
    border-top: 10px solid #ECECEC;
    margin-top: 20px;
    padding-top: 10px;
    overflow: hidden;
}
#OwDemoSearchForm #OwDemoSearchForm_Header
{
    overflow: hidden;
}
