/* **********************************************
    Customize the navigation column
************************************************* */
#OwSubLayout_NAVIGATION,
.com_wewebu_ow_RecordArchiv #OwSubLayout_Div #OwSubLayout_NAVIGATION,
.OwRecordSubLayout_NAVIGATION .OwMainColumnHeader,
#OwSubLayout_NAVIGATION #OwRecordPreviewPropertyView
{
    width: 235px;
}
*:first-child+html ul#OwSubNavigationView li a
{
    width: 223px; /* set the correct width for ie7, it must be 12px less then in other browsers */
}
*:first-child+html .OwSubLayoutDynamic_NAVIGATION ul#OwSubNavigationView li a
{
	width: auto; /* for dynamic subNavigation, use auto width */
}

*:first-child+html #OwSubLayout_NAVIGATION .OwTreeView
{
    width: 226px; /* set the correct with for ie7 */  
}
#OwSubLayout_Div,
{
	background-image: none;
	border: 1px solid #ebebeb;
	border-top: none;
}
#OwSubLayout_Div #OwSubLayout_NAVIGATION
{
	border-right: 3px solid #ebebeb;
	border-top: 1px solid #8F9BAC;
	padding: 0px;
	min-height: 400px;
}
#OwSubLayout_Div #OwSubLayout_MAIN,
.OwStandardDialog_MAIN #OwSubLayout_MAIN
{
    margin-left: 240px; /* set the left margin of the main content column */
}

/* define layouts without navigation here */
.com_wewebu_ow_Help div#OwSubLayout_Div #OwSubLayout_MAIN,
.com_wewebu_ow_Admin div#OwSubLayout_Div #OwSubLayout_MAIN,
.com_wewebu_ow_Admin .OwStandardDialog_MAIN #OwSubLayout_MAIN,
.com_wewebu_ow_RecordArchivRegister div#OwMainLayout_MAIN div#OwSubLayout_Div div.OwSubLayoutRegister_MAIN,
.com_wewebu_ow_RecordArchivDossierRegister div#OwMainLayout_MAIN div#OwSubLayout_Div div.OwSubLayoutRegister_MAIN,
.com_wewebu_ow_New div#OwSubLayout_Div #OwSubLayout_MAIN
{
    margin-left: 0;
    background-color: white;
}

/* **********************************************
    Customize the block label layout.
    This is used for forms in table layout without 
    using HTML tables (accessibility)
************************************************* */
.OwPropertyBlockLayout .OwPropertyLabel,
form label.OwPropertyName,
form .OwPropertyControlBlockLabelLayout label,
form .blockLabel label
{
    width: 215px;
}
.OwNowrapLabel .OwPropertyLabel,
form table.OwNowrapLabel td.OwPropertyName
{
    min-width: 215px;
}
*:first-child+html form table.OwNowrapLabel td.OwPropertyName label
{
    width: 215px;
}

/* the label width can be overridden in special views, 
   e.g. in OwBPMWorkItemListView_SEARCH_CRITERIA */
.OwBPMWorkItemListView_SEARCH_CRITERIA .OwNowrapLabel .OwPropertyLabel
{
    min-width: 120px;
}

