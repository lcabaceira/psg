#OwMainLayout_NAVIGATION ul li.OwMainNavigationView_notavailable a
{
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#5e7ebb), color-stop(100%,#698cd1)); 
    background-image: -webkit-linear-gradient(to bottom, #5e7ebb 0px, #698cd1 100%);
    background-image: -moz-linear-gradient(to bottom, #5e7ebb 0px, #698cd1 100%);
    /*background-image: -o-linear-gradient(to bottom, #5e7ebb 0px, #698cd1 100%);*/
    background-image: -ms-linear-gradient(to bottom, #5e7ebb 0%, #698cd1 100%); /* IE10+ */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#5e7ebb', endColorstr='#698cd1',GradientType=0 );
    background-image: linear-gradient(to bottom, #5e7ebb 0%, #698cd1 100%);
    color: #ffffff;
    border-right: 1px solid #ffffff;
    border-left: 1px solid #ffffff;
    margin-bottom: -1px;
    position: relative;
    text-decoration: underline;	
}

